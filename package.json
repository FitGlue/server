{
  "name": "fitglue-server",
  "version": "12.2.0",
  "description": "FitGlue server-side Cloud Functions and API handlers",
  "dependencies": {
    "ts-proto": "^2.8.3"
  },
  "scripts": {
    "release": "standard-version --releaseCommitMessageFormat \"chore(release): {{currentTag}} [skip ci]\"",
    "test:integration": "jest",
    "test:local": "TEST_ENVIRONMENT=local jest --testPathPatterns=local.test.ts",
    "test:deployed": "jest --testPathPatterns=deployed.test.ts",
    "test:dev": "TEST_ENVIRONMENT=dev npm run test:deployed",
    "test:test": "TEST_ENVIRONMENT=test npm run test:deployed",
    "build": "echo 'No build script yet'"
  },
  "devDependencies": {
    "@google-cloud/pubsub": "^4.10.0",
    "@google-cloud/secret-manager": "^6.1.1",
    "@google-cloud/storage": "^7.18.0",
    "@types/jest": "^30.0.0",
    "axios": "^1.13.2",
    "firebase-admin": "^13.6.0",
    "jest": "^30.2.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.4.6",
    "ts-morph": "^27.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "uuid": "^13.0.0"
  }
}
